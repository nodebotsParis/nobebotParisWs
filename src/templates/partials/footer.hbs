<div class="footerCont">
  <div class="container">
    <div class="row footer">
      <div class="col-md-3">
        <h4>Sitemap</h4>
        <ul class="sitemap bottomNav">
          {{#each site.menu}}
          <li>
            <a  href ="{{#if url}} {{url}} {{else}}#{{/if}}">
              {{title }}
            </a >
          </li>
          {{/each}}
        </ul>
      </div>
      <div class="col-md-3">
        <h4>Amis</h4>
        <ul class="bottomNav">

          {{#each site.friends}}
          <li>
            <a  href ="{{#if url}} {{url}} {{else}}#{{/if}}">
              {{title }}
            </a >
          </li>
          {{/each}}
        </ul>
      </div>
      <div class="col-md-3">
        <h4>Contacts</h4>
        <ul class="bottomNav">
          {{#each site.contacts}}
          <li>
            <a  href ="{{#if url}} {{url}} {{else}}#{{/if}}">
              {{title }}
            </a >
          </li>
          {{/each}}
        </ul>
      </div>
      <div class="col-md-3 taC">
        <img src="{{assets}}/images/nodebotsvial.png" alt="">
      </div>
    </div>
  </div>
</div>
